class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int t) {
    
          vector<int> target;
          for(int i=0; i<nums.size();i++)
          {    
              for(int j=0; j<nums.size();j++)
              {
                  if(nums[i] + nums[j] == t && i != j)
                  {
                      target.push_back(i);
                      target.push_back(j);
                      return target;
                  }
              }
              
              return target;
           }
        }
